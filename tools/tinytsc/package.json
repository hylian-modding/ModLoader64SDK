{
  "name": "tinytsc",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npx tsc",
    "copy": "run-script-os",
    "copy:test:win32": "copy tinytsc-win.exe ..\\..\\tinytsc.exe",
    "copy:nix": "chmod +x ./tinytsc-linux && cp ./tinytsc-linux ../../tinytsc",
    "dist": "cp ./tinytsc-win.exe ../../dist/windows/tinytsc.exe && cp ./tinytsc-linux ../../dist/linux/tinytsc",
    "pkg": "pkg --compress GZip . && yarn copy",
    "postinstall": "yarn build && yarn pkg"
  },
  "bin": {
    "tinytsc": "./build/index.js"
  },
  "author": "",
  "license": "ISC",
  "pkg": {
    "scripts": "build/**/*.js",
    "assets": [
      "../../node_modules/**/*",
      "node_modules/**/*"
    ],
    "targets": [
      "node16-win-x64",
      "node16-linux-x64"
    ],
    "outputPath": "."
  },
  "dependencies": {
  }
}
