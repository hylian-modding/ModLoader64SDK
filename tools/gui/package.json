{
  "name": "modloader64-sdk-gui",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "../../tinytsc",
    "copy": "run-script-os",
    "copy:test:win32": "cp ./modloader64-sdk-gui-win.exe ../../modloader64-sdk-gui.exe",
    "copy:nix": "chmod +x ./modloader64-sdk-gui-linux && cp ./modloader64-sdk-gui-linux ../../modloader64-sdk-gui",
    "dist": "cp ./modloader64-sdk-gui-win.exe ../../dist/windows/modloader64-sdk-gui.exe && cp ./modloader64-sdk-gui-linux ../../dist/linux/modloader64-sdk-gui",
    "pkg": "pkg --compress GZip .",
    "postinstall": "yarn build && yarn pkg && yarn copy && yarn dist"
  },
  "bin": {
    "modloader64-sdk-gui": "./build/index.js"
  },
  "author": "",
  "license": "ISC",
  "pkg": {
    "scripts": "build/**/*.js",
    "assets": [
      "../../node_modules/**/*",
      "node_modules/**/*"
    ],
    "targets": [
      "node16-win-x64",
      "node16-linux-x64"
    ],
    "outputPath": "."
  },
  "dependencies": {
    "ml64tk": "https://github.com/hylian-modding/ml64tk-v1"
  }
}
