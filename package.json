{
  "name": "modloader64",
  "version": "2.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "postinstall": "npx patch-package",
    "build": "yarn && ts-node ./prebuild.ts && ./tinytsc && yarn pkg && yarn copy",
    "copy": "run-script-os",
    "copy:test:win32": "cp ./modloader64-win.exe ./modloader64.exe",
    "copy:nix": "cp ./modloader64-linux ./modloader64 && chmod +x ./modloader64 && cp ./modloader64 ./ModLoader64/modloader64",
    "dist": "yarn updater && yarn gui && mv ./modloader64-win.exe ./dist/windows/modloader64.exe && mv ./modloader64-linux ./dist/linux/modloader64 && ts-node ./dist.ts",
    "pkg": "pkg --compress GZip .",
    "paker": "cd ./tools/paker && yarn",
    "bintots": "cd ./tools/bintots && yarn",
    "linker": "cd ./tools/linker && yarn",
    "recompiler": "cd ./tools/modrecompiler && yarn",
    "tinytsc": "cd ./tools/tinytsc && yarn",
    "updater": "cd ./tools/updater && yarn",
    "gui": "cd ./tools/gui && yarn",
    "tools": "yarn tinytsc",
    "update_core": "ts-node ./updateml.ts",
    "fullbuild": "yarn && yarn tools && yarn update_core && yarn build && yarn dist",
    "docker": "yarn fullbuild && cp -R ./dist /output",
    "MLPreBuildScript": "./prebuild.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/adm-zip": "^0.5.0",
    "@types/fs-extra": "^9.0.13",
    "@types/gulp": "^4.0.9",
    "@types/node": "^18.0.6",
    "adm-zip": "^0.5.9",
    "asar": "^3.1.0",
    "asar-node": "^3.1.0",
    "commander": "^9.3.0",
    "fs-extra": "^10.1.0",
    "lzma": "^2.3.2",
    "mkdir-recursive": "^0.4.0",
    "tree-kill": "^1.2.2",
    "typescript": "^4.7.4",
    "uprocess": "^1.1.0"
  },
  "bin": {
    "modloader64": "./build/index.js"
  },
  "pkg": {
    "scripts": "build/**/*.js",
    "assets": [
      "node_modules/**/*",
      "build/windows.zip",
      "build/windows.md5",
      "build/linux.zip",
      "build/linux.md5"
    ],
    "targets": [
      "node16-win-x64",
      "node16-linux-x64"
    ],
    "outputPath": "."
  },
  "devDependencies": {
    "run-script-os": "^1.1.6"
  }
}
